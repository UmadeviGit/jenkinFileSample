node {
    stage('Build') {
         print "Hello cmake"
       
     dir( ""${WORKSPACE}")
       {
       sh "ls"
       sh "cmake -S . -B build" 
       }

        echo 'Building....'
    }
    stage('Test') {
     print "Hello world"
        echo 'Testing....'
    }
    stage('Deploy') {
     print "Hello world 123"
        echo 'Deploying....'
    }
}
