node {
    stage('Build') {
         print "Hello"
         def out = exec("cp866", "C:\\ProgramData\\Jenkins\\.jenkins\\workspace\\Sample1", "cmd", ["cmake","-S",".","-B","build","CMakeLists.txt"])

        println "OUT:\n" + out[0]
        println "ERR:\n" + out[1]
        echo 'Building....'
    }
    stage('Test') {
     print "Hello world"
        echo 'Testing....'
    }
    stage('Deploy') {
     print "Hello world 123"
        echo 'Deploying....'
    }
}
