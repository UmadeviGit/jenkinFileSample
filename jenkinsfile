node {
    stage('Build') {
         print "Hello"
        File workingDir = file("C:\\ProgramData\\Jenkins\\.jenkins\\workspace\\Sample1")
        def proc = "cmake -S . -B build".execute([], workingDir.absolutePath);
        echo 'Building....'
    }
    stage('Test') {
     print "Hello world"
        echo 'Testing....'
    }
    stage('Deploy') {
     print "Hello world 123"
        echo 'Deploying....'
    }
}
