node {
    stage('Build') {
         print "Hello"
       
       
        
        def command = "cd pwd"
def proc = command.execute()
proc.waitFor()   

def commando = "cmake -S . -B build"
def procs = commando.execute()
procs.waitFor()   

        echo 'Building....'
    }
    stage('Test') {
     print "Hello world"
        echo 'Testing....'
    }
    stage('Deploy') {
     print "Hello world 123"
        echo 'Deploying....'
    }
}
