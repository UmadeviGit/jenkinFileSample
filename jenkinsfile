node {
pipeline {
    agent { docker { image 'python:3.10.1-alpine' } }
    
    stage('Build') {
         print "Hello cmake"
       
     dir("${WORKSPACE}")
       {
       sh "ls"
       sh "python Hello.py" 
       }

        echo 'Building....'
    }
    stage('Test') {
     print "Hello world"
        echo 'Testing....'
    }
    stage('Deploy') {
     print "Hello world 123"
        echo 'Deploying....'
    }
}
}
